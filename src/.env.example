# FTP Server Configuration
# Replace these values with your actual FTP server details

# FTP server IP address or hostname
FTP_HOST = "ftp.example.com"

# FTP username for authentication
FTP_USER = "your_username"

# FTP password for authentication
FTP_PASS = "your_password"

# Optional: FTP port (uncomment if using non-standard port)
# FTP_PORT = "21"

# Optional: SSL/TLS settings (uncomment if using secure FTP)
# FTP_USE_TLS = "True"
# FTP_CERT_PATH = "/path/to/cert.pem"
# FTP_KEY_PATH = "/path/to/key.pem" 